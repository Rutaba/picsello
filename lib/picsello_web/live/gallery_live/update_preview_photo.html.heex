<div>
  <.form let={f} for={@changeset} phx-change="validate"
   phx-submit="save" phx-target={@myself} id="updateGalleryPreviewPhoto">
    <%= hidden_input f, :url %>

    <%= submit "Save", class: "btn-primary mt-5 px-11 py-3.5 float-right cursor-pointer", disabled: !@changeset.valid?, phx_disable_with: "Saving..." %>
  </.form>
  
</div>